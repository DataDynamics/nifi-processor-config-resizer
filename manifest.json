{
  "manifest_version": 3,
  "name": "__MSG_ext_name__",
  "version": "1.0.0.24",
  "default_locale": "ko",
  "description": "__MSG_ext_desc__",
  "icons": {
    "16": "icons/nifi16.ico",
    "48": "icons/nifi48.png",
    "128": "icons/nifi128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/nifi16.ico",
      "48": "icons/nifi48.png",
      "128": "icons/nifi128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*:8080/nifi/*"
      ],
      "js": [
        "resizer.js"
      ]
    }
  ],
  "homepage_url": "https://github.com/DataDynamics/nifi-processor-config-resizer"
}
